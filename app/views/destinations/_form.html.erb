<%= form_for [@list, @destination] do |f| %>
  <%= f.label :country %>
  <%= f.text_field :country, required: true %>
  <br />
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br />
  <%= f.label :img_url %>
  <%= f.text_field :img_url, placeholder: 'http://image.jpg' %>
  <br />
  <%= f.submit %>
<% end %>
